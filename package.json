{
  "name": "express_template",
  "version": "0.0.0",
  "description": "MVC express template with CRUD Implementation for free",
  "main": "./server.js",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/BrittleHeart/Blog"
  },
  "author": "Bartosz Pazdur",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node dist/server.bundle.js",
    "dev": "nodemon dist/server.bundle.js -q",
    "test": "mocha",
    "watch_server": "webpack --watch --mode development --config webpack.config.js",
    "watch_react": "webpack --watch --mode development --config webpack.react.config.js",
    "build_server": "webpack --mode=production --config webpack.config.js",
    "build_react": "webpack --mode=production --config webpack.react.config.js",
    "production": " NODE_ENV=production && rm -rf ./node_modules && rm -rf ./dist && rm package-lock.json && npm i && npm run build_server && npm run build_react"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-handlebars": "^5.1.0",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.12",
    "mongoose-timestamp": "^0.6.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^2.2.1",
    "husky": "^3.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 9.0.0",
    "yarn": "^0.13.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
